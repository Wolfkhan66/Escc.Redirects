@{
    ViewBag.Title = "Moved Page";
}
@model System.Data.DataTable
@using System.Data;
<head>
    <link href="~/Content/DataTables/css/jquery.dataTables.min.css" rel="stylesheet" />
    <script src="~/Scripts/DataTables/jquery.dataTables.min.js"></script>
</head>

<div class="jumbotron alert-info">
    <h1>Moved Pages</h1>
    <p class="lead">
        These pages and sections on the eastsussex.gov.uk have moved, but visitors are redirected to the new location.
        If the page requested by the visitor starts with the previous location shown below, that's enough. It doesn't have to be an exact match.
    </p>
    <p><a class="btn btn-info" href="/Home/Add?Id=2">Add Redirect &raquo;</a></p>
</div>

<style media="screen" type="text/css">
    table.dataTable tbody td {
        word-break: break-word;
        vertical-align: top;
    }
</style>

<div class="row">
    <div class="table-responsive">
        <table class="table table-striped table-bordered display" cellspacing="0" width="100%" id="contentTable">
            <thead>
                <tr>
                    @foreach (DataColumn col in Model.Columns)
                    {
                        <th>@col.ColumnName</th>
                    }
                </tr>
            </thead>
            <tbody>
                @foreach (DataRow row in Model.Rows)
                {
                    <tr>
                        @foreach (DataColumn col in Model.Columns)
                        {
                            <td>@row[col.ColumnName]</td>
                        }
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>

<script>
    $('#contentTable').DataTable();
</script>
